.relative
    .rs-fullscr-container
        #rs-fullwidth.tp-banner.dark-bg
            ul
                // SLIDE 1
                li(data-transition='zoomout', data-slotamount='1', data-masterspeed='1500', data-saveperformance='on')
                    // MAIN IMAGE
                    img(src='images/1.jpg', alt='slidebg1', data-bgposition='center top', data-bgfit='cover', data-bgrepeat='no-repeat')
                    // LAYERS
                    // PARALLAX & OPACITY container
                    .rs-parallaxlevel-4.opacity-scroll2
                        // LAYER NR. 1 - TOP LINE (smaller, lighter)
                        .tp-caption.font-white.light-60-wide.sfb.tp-resizeme(data-x='center', data-hoffset='0', data-y='center', data-voffset='-35', data-speed='500', data-start='850', data-easing='Power1.easeInOut', data-splitin='none', data-splitout='none', data-elementdelay='0.1', data-endelementdelay='0.1', style='z-index: 7; max-width: auto; max-height: auto; white-space: nowrap;')
                            span Secure Your Digital Future
                        // LAYER NR. 2 - BOTTOM LINE (larger, bolder)
                        .tp-caption.font-white.light-60-wide.sfb.tp-resizeme(data-x='center', data-y='center', data-voffset='35', data-speed='500', data-start='1050', data-easing='Power1.easeInOut', data-splitin='none', data-splitout='none', data-elementdelay='0.1', data-endelementdelay='0.1', style='z-index: 8; max-width: auto; max-height: auto; white-space: nowrap;')
                            span.bold With Bulletproof Cybersecurity
                        // CTA BUTTONS
                        .tp-caption.font-white.light-60-wide.sfb.tp-resizeme(data-x='center', data-y='center', data-voffset='100', data-speed='500', data-start='1250', data-easing='Power1.easeInOut', data-splitin='none', data-splitout='none', data-elementdelay='0.1', data-endelementdelay='0.1', style='z-index: 9; max-width: auto; max-height: auto; white-space: nowrap;')
                            .hero-cta-buttons
                                a.btn.btn-lg.btn-color.mr-20(href='#quote-request') Request Free Security Audit
                                a.btn.btn-lg.btn-white.ml-20(href='#quote-request') Book Prototype Review Call
