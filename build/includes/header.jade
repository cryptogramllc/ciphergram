div(id="home-link")
    header#nav.header.header-1.black-header.mobile-no-transparent
        .header-wrapper
            .container-m-30.clearfix
                .logo-row
                    // LOGO
                    .logo-container-2
                        .logo-2
                            a.clearfix(href='index.html')
                                include ./logo.jade
                    // BUTTON
                    .menu-btn-respons-container
                        button.navbar-toggle.btn-navbar.collapsed(type='button', data-toggle='collapse', data-target='#main-menu .navbar-collapse')
                            span.icon_menu.hamb-mob-icon(aria-hidden='true')
            // MAIN MENU CONTAINER
            .main-menu-container
                .container-m-30.clearfix
                    // MAIN MENU
                    #main-menu
                        .navbar.navbar-default(role='navigation')
                            // MAIN MENU LIST
                            nav.collapse.navbar-collapse.right-1024
                                ul.nav.navbar-nav(id="nav-onepage")
                                    // MENU ITEM
                                    li.current
                                        a(href='#home-link')
                                            .main-menu-title HOME
                                    li
                                        a(href='#about-us-link')
                                            .main-menu-title ABOUT
                                    li
                                        a(href='#services-link')
                                            .main-menu-title SERVICES
                                    li
                                        a(href='#our-work-link')
                                            .main-menu-title OUR WORK
                                    li
                                        a(href='#contact-link')
                                            .main-menu-title CONTACT
                    // END main-menu
                // END container-m-30
            // END main-menu-container
            // SEARCH READ DOCUMENTATION
            ul.cd-header-buttons
                li
                    a.cd-search-trigger(href='#cd-search')
                        span
            // cd-header-buttons
            #cd-search.cd-search
                form#searchForm.form-search(action='page-search-results.html', method='get')
                    input#q(type='text', value='', name='q', placeholder='Search...')
        // END header-wrapper